--4.13  Tram Ride Ticket Type Table

CREATE TABLE TR_Ticket_Type (
ticket_type	varchar(8)	not null,
price			number(4,2),
primary key(ticket_type)
);

SET COLSEP '|'
COLUMN ticket_type FORMAT A12 HEADING 'Ticket Type';
COLUMN price FORMAT 99.99 HEADING 'Price';


