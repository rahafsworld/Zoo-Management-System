COLUMN job_position FORMAT A20;
COLUMN phone_number FORMAT A13;
COLUMN email FORMAT A30;

SET LINESIZE 120;
SET PAGESIZE 35;


--4.2 Animal Table

CREATE TABLE animal (
    animal_id VARCHAR(10) NOT NULL,
    animal_name VARCHAR(50) UNIQUE,
    species VARCHAR(50),
    gender CHAR(1),
    age INT,
    enclosure_id VARCHAR(4),
    PRIMARY KEY(animal_id),
    CHECK (enclosure_id IN ('E001', 'E002', 'E003', 'E004', 'E005', 'E006', 'E007', 'E008', 'E009', 'E010'))
);

