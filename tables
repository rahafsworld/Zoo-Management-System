--4.15 Animal Show Participation Table

CREATE TABLE Animal_Show_Participation (
show_id	varchar(6)	not null,
animal_id	varchar(5)	not null,
primary key(show_id, animal_id),
foreign key (animal_id) references animal(animal_id),
foreign key (show_id) references Animal_Show(show_id)
);

SET COLSEP '|'
COLUMN show_id FORMAT A7 HEADING 'Show ID';
COLUMN animal_id FORMAT A9 HEADING 'Animal ID';


