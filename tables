--4.9 Maintenance Table

SET COLSEP '|'
SET LINESIZE 100;
SET PAGESIZE 50;
COLUMN maintenance_id FORMAT A10;
COLUMN staff_id FORMAT A10;
COLUMN enclosure_id FORMAT A10;
COLUMN status FORMAT A15;
COLUMN maintenance_type FORMAT A30;
COLUMN maintenance_frequency FORMAT A15;

CREATE TABLE maintenance (
    maintenance_id        VARCHAR2(10)	PRIMARY KEY,
    staff_id              VARCHAR2(10),
    enclosure_id          VARCHAR2(10),
    status                VARCHAR2(20),
    maintenance_type      VARCHAR2(30),
    maintenance_frequency VARCHAR2(15),
    CONSTRAINT fk_maintenance_staff
        FOREIGN KEY (staff_id)
        REFERENCES staff(staff_id),
    CONSTRAINT fk_maintenance_enclosure
        FOREIGN KEY (enclosure_id)
        REFERENCES enclosure(enclosure_id)
);

