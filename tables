set linesize 120
set pagesize 50
CREATE TABLE Tram_Ride (
ride_id	varchar(6)	not null,
status		varchar(10),
ride_date	date,
tram_id	varchar(5),
primary key(ride_id),
foreign key(tram_id) references Tram(tram_id)
);

