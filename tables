--4.3 Visitor Table

CREATE TABLE visitor (
    visitor_id VARCHAR(12) NOT NULL,
    visitor_name VARCHAR(100),
    email VARCHAR(100) UNIQUE,
    phone_number VARCHAR(20) UNIQUE,
    address VARCHAR(255),
    age INT,
    gender CHAR(1),
    PRIMARY KEY(visitor_id)
);


