--4.22 Purchase Item Table

CREATE TABLE purchase_item (
purchase_id	 VARCHAR2(7) NOT NULL,	
shop_item_id	 VARCHAR2(8) NOT NULL,
quantity	 NUMBER(3),
PRIMARY KEY(purchase_id, shop_item_id),
FOREIGN KEY(purchase_id) REFERENCES purchase(purchase_id),
FOREIGN KEY(shop_item_id) REFERENCES shop_item(shop_item_id)
);


